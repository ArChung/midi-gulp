  <link rel="image_src" href="./share.jpg" />

  <!-- OG -->
  <meta property="og:title" content="<%- title %>" />
  <meta property="og:site_name" content="<%- title %>" />
  <meta property="og:description" content="獨家代理印度知名品牌 Double Deer，來自印度喜馬拉雅山下的頂級印度香米，長度超過 8.4 mm，全台灣印度餐廳都在用" />
  <meta property="og:type" content="website" />
  <meta property="og:image" content="./share.jpg" />
  <meta property="og:url" content="" />
  <!-- CSS -->
  <!-- build:css css/lib.css -->
  <!-- endbuild -->
  <link rel="stylesheet" href="css/all.css">
</head>

<body>
  <!-- sider -->
  <% include conponents/sider %>
  <!-- sider END-->

  <!-- navbar -->
  <% include conponents/header %>
  <!-- navbar END -->

  <div id='mainContainer'>
    <!-- contents -->
    <%- contents %>
    <!-- contents END -->
  </div>

  <!-- footer -->
  <% include conponents/footer %>
  <!-- footer END -->


  <!-- build:js js/lib.js -->
  <script type="text/javascript" src="js/lib/jquery.min.js"></script>
  <script type="text/javascript" src="js/lib/fastclick.js"></script>
  <script type="text/javascript" src="js/lib/in-view.min.js"></script>
  <script type="text/javascript" src="js/lib/TweenMax.min.js"></script>
  <script type="text/javascript" src="js/lib/TimelineMax.min.js"></script>
  <!-- endbuild -->
  <!-- js -->
  <script src="js/all.js"></script>
  <!-- js END-->
</body>

</html>